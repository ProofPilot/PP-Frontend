{% if studyCode is not defined %}
    {% set studyCode = app.request.get('studyCode') %}
{% endif %}
<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js">
  <!--<![endif]-->
  <head>
    <meta content="text/html; charset=utf-8" http-equiv="content-type">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    
    <title>{% if branding != 'knowathome' %}
    			{% block title %}
	                {% if studycontent is defined %}
	                    {{ studycontent.studyName }}
	                {% else %}
	                     - ProofPilot
	                {% endif %}
                {% endblock title %}
           {% else %}
                KnowAtHome 
                {% endif %}
           </title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href="//fonts.googleapis.com/css?family=Roboto:300" rel="stylesheet" type="text/css" />
    <link href="//fonts.googleapis.com/css?family=Roboto:400" rel="stylesheet" type="text/css" />
    <link href="//fonts.googleapis.com/css?family=Roboto:500" rel="stylesheet" type="text/css" />
    <link href="//fonts.googleapis.com/css?family=Roboto:700" rel="stylesheet" type="text/css" />
    <link href="//fonts.googleapis.com/css?family=Roboto:900" rel="stylesheet" type="text/css" />
    <link rel="icon" href="{{ asset('images/favicon.ico') }}" type="image/x-icon"/>
    <link rel="shortcut icon" {% if branding != 'knowathome' %} href="{{ asset('images/favicon.ico') }}" {% else %} href="{{ asset('images/knowathome_favicon.ico') }}" {% endif %} type="image/x-icon"/>
    <link rel="stylesheet" href={{ asset('css/jquery-ui-1.10.3/base/jquery-ui.min.css') }} />
    <link rel="stylesheet" href={{ asset('css/normalize.css') }} />
    <link rel="stylesheet" href={{ asset('css/main.css') }} />
    
    {% stylesheets
        'css/site.css'
        'css/mobile.css'
        'css/media-queries.css'
        filter='cssrewrite'
        output='generated/global.css'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    
    {% block stylesheets %}
    {%  endblock stylesheets %}
    
    <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    <script src={{ asset('js/vendor/modernizr-2.6.2.min.js') }}></script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script> 
    <script>window.jQuery || document.write('<script src={{ asset('js/vendor/jquery-1.10.2.min.js') }}><\/script>')</script>
    <script src={{ asset('js/jquery-ui-1.10.3/jquery-ui.min.js') }}></script>
    <script src= {{ asset ('js/plugins.js') }}></script>
    <script src= {{ asset ('js/main.js') }}></script>
  
    
    <script> var ISOLDIE = false;</script>
    <!--[if lt IE 9]>
        <script> var ISOLDIE = true; </script>
    <![endif]-->
    <script>
    var browser_msg = "{% trans from "messages" %}browser_msg{% endtrans %}";
    
    if(ISOLDIE) {
        alert(browser_msg);
    }
    </script>
    {% block javascripts %}
    {% endblock javascripts %}

  </head>

  {% block body %}
  <body class="has_image" {{ study_background(branding, random_themes)|raw }}>
      {% block scripts_facebook %}{% endblock scripts_facebook %}
      <div class="block_wrapper">
      {% include '::header_reg.html.twig' %}
      {% block content %}
      {% endblock content %}
      <div class="push"></div>
      </div>
      {% include '::footer_reg.html.twig' %}
      {% block scripts_google %}{% endblock scripts_google %}
  </body>
  {% endblock body %}

</html>
