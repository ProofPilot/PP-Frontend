{%  extends '::base.html.twig' %}{% block body %}  <body class="has_image">    <div class="reg_header"> <a href="#" class="logo">ProofPilot</a> </div>    <div class="mobile_reg">      <h1><span>JOIN 4/4:</span> Enter code you received via SMS </h1>      <br>      <form method="post" action="{{ path('reg_step_4') }}">        <ul class="form_fields">          <li>          {% if form is defined %}              {{ form_widget(form.sms_code, {'attr' : {'class' : 'default_input full'}}) }}              {{ form_errors(form.sms_code) }}              {{ form_errors(form) }}              {{ form_rest(form) }}          {% else %}          <input name="" value="" class="default_input full" type="text">          {% endif %}          {{ error }}          </li>        </ul>        <div class="center_align"><a href="main.html" class="submit" id="register_submit">Confirm            code &amp; login</a></div>            <button type="submit" style="display:none" id="submit_button"></button>        <div class="center_align"><a href={{ path('reg_step_3') }} class="repeat">No code? Send            another</a></div>      </form>    </div>    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>        <script>window.jQuery || document.write('<script src={{ asset('js/vendor/jquery-1.9.1.min.js') }}><\/script>')</script>        <script src= {{ asset ('js/plugins.js') }}></script>        <script src= {{ asset ('js/main.js') }}></script>        <script>                $(document).ready(function(){            $('#register_submit').click(function()                {                         $('#submit_button').trigger('click');                        return false;                });           });        </script>  </body>{% endblock body %}