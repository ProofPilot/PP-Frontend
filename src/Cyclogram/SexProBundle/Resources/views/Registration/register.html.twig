{%  extends '::base.html.twig' %}{% form_theme form _self %}{# form_errors.html.twig #}{% block form_errors %}    {% spaceless %}        {% if errors|length > 0 %}            {% for error in errors %}                <p class="error">{{ error.message }}</p>            {% endfor %}        {% endif %}    {% endspaceless %}{% endblock form_errors %}{% block form_widget_simple %}{% spaceless %}    {% set type = type|default('text') %}    {% if errors %}        {% set class_error = attr.class ~ " has_error" %}        {% set attr = attr|merge({'class': class_error}) %}     {% endif %}    <span class="input_placeholder {% if errors %}has_error{% endif %}">        <input type="{{ type }}" {{ block('widget_attributes') }} {% if value is not empty %}value="{{ value }}" {% endif %}/>        <span class="mark"></span>    </span>{% endspaceless %}{% endblock form_widget_simple %}{% block javascripts %}<script>       function goLogIn(){           window.location = "{{ path('_fb_security_check') }}";       }       function onFbInit() {           if (typeof(FB) != 'undefined' && FB != null ) {               FB.Event.subscribe('auth.statusChange', function(response) {                   setTimeout(goLogIn, 500);               });           }           FB.Event.subscribe('auth.logout',                    function(response) {                        if (response.status === 'unknown')  {                           window.location = "{{ path('_logout') }}";                         }           });                                   }</script>{% endblock javascripts %}{% block body %}<body class="has_image">  {{ facebook_initialize({'xfbml': true, 'fbAsyncInit': 'onFbInit();'}) }}    <div class="reg_header"> <a href="#" class="logo">ProofPilot</a> </div>    <div class="mobile_reg login_form">      <div class="wrapper">        <h1><span>JOIN 1/4:</span> The Basics</h1>        <div class="loginform_left register">          <div class="btns_wapper">            <form>             {{ facebook_login_button({'autologoutlink': true}) }}{#             <a class="btn_reg fb" href="#"><span>Join with Facebook</span></a>#}            </form>            <form> <a id="expand_google_form" class="btn_reg google" href="#"><span>Join                  with Google</span></a> </form>          </div>          <div class="notice2">            <p><span>Why</span></p>          </div>        </div>        <div class="sep wide"> OR </div>        <div class="loginform_right">          <form action="{{ path('_registration') }}" method="post" name="registration">                         <label for="reg_field_1">Enter your e-mail</label>               {{ form_widget(form.participantEmail, {'attr' : {'class' : 'text', 'id' : 'reg_field_1'}}) }}              {{ form_errors(form.participantEmail) }}              <label for="reg_field_2">Pick Username</label>               {{ form_widget(form.username, {'attr' : {'class' : 'text', 'id' : 'reg_field_2'}}) }}{#               <input class="text" value="" name="" id="reg_field_2" type="text"></span>#}              <label for="reg_field_3">Create a Password</label>              <span class="note">(8 Characters min)</span>               {{ form_widget(form.participantPassword.first, {'attr' : {'class' : 'text', 'id' : 'reg_field_3'}}) }}                            <label for="reg_field_4">Confirm Password</label>              {{ form_widget(form.participantPassword.second, {'attr' : {'class' : 'text', 'id' : 'reg_field_4'}}) }}                            {{ form_rest(form) }}              {{ form_errors(form) }}                                               <div class="actions_row"> <a href="#" class="submit with_min_width btn_login" id="register_submit">NEXT&gt;</a>               <button type="submit" style="display:none" id="submit_button"></button></div>                        </form>          <div class="mobile_text">Allready Joined? <a href="#">Log in</a></div>        </div>      </div>    </div>    <div class="login_btn_placeholder"> Allready Joined? <a href="#">Log in</a>    </div>    <script type="text/javascript">        $(document).ready(function(){            $('#register_submit').click(function() {                $('#submit_button').trigger('click');                return false;            });            $('#facebook_button').click(function() {                 FB.login(null, {scope: 'email'});            });        });    </script>   </body>{% endblock body %}