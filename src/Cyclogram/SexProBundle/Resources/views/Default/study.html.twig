{% extends '::base.html.twig' %}{% use '::general.html.twig' %}{% block content %}  {% include '::header_blank.html.twig' %}    <div class="mobile_header_menu">    <ul>      <li><a href="#" class="trigger_left_menu"></a></li>      <li class="logo"><a href="#" class="site_logo">ProofPilot</a></li>      <li class="right"><a href="#" class="trigger_sitemenu"></a></li>    </ul>  </div>  <div class="mobile_sitemenu">    <div class="menu_wrapper">      <a href="#" class="close"></a> <a href="#" class="logo">ProofPilot</a>      <ul>        <li><a class="icon_settings" href="#"><span>My Settings</span></a></li>        <li><a class="icon_logout" href="#"><span>Logout</span></a></li>        <li><a class="icon_help" href="#"><span>Help</span></a></li>      </ul>    </div>  </div>  <div class="panel alt_bg">    <div class="panel_left study">      <br/>      <div class="menu_cont">        <ul class="left_menu">          <li><a href="#" class="icon_dashboard active">Dashboard <span class="notice">5</span></a></li>          <li class="first"><a class="icon_survey" href="#">Survey</a></li>          <li><a class="icon_activities" href="#">Activities</a></li>          <li><a class="icon_measurments" href="#">Measurments</a></li>          <li><a class="icon_treatment" href="#">Treatment</a></li>        </ul>      </div>      <div class="study_desc">        <h3>{% trans from "study" %}txt_about{% endtrans %}</h3>          <span style="color: #7e7e7e;">{{ about.info }}</span>      </div>    </div>    <div class="panel_right">      <div class="dashboard_title green">        <h1>{% trans from "study" %}txt_you_are_eligible{% endtrans %}</h1>        <p>Your participation at a glance: What you need to do now and whatâ€™s next.</p>      </div>      <div class="dashboard_panel wider">        <ul class="study_info">          <li class="image"> <img src="/images/tmp_sexpro.png" alt="" title="" height="103" width="103"></li>          <li class="text">            <h1>{{ study.title }}</h1>          </li>        </ul>        <div class="mobile s_desc">          <p>Surveys allow you to provide feedback, input information all online.</p>        </div>        <div class="mobile about_box"> <a href="#" class="expander" id="about_expander"><span>{{ about.title }}</span></a>          <div class="text_desc">            <p>{{ about.info }}</p>          </div>        </div>        <p class="notice gray"><span>{% trans from "study" %}warning_message{% endtrans %}</span></p>        <div class="img_placeholder"><img src="/images/tmp_img.png" alt="" title="" height="324" width="687"></div>        <p>{{ study.description }}</p>        {% for block in content %}        <h2>{{ block.title }}</h2>        <p style="color: #7e7e7e;">{{ block.text }}</p>        {% endfor %}        <p><br/></p>      </div>      <div class="btns_box for_mobile inlined">        <ul>          <li class="red"><a href="page.html" class="red left"><span>{% trans from "study" %}txt_decline_btn{% endtrans %}</span></a></li>          <li class="green"><a href="register.html" class="green"><span>{% trans from "study" %}txt_accept_btn{% endtrans %}</span></a></li>        </ul>      </div>    </div>  </div>{% endblock content %}