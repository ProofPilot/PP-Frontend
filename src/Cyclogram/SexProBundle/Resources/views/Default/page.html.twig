{% extends '::base.html.twig' %}{% use '::general.html.twig' %}{% block content %}  {% include '::header_menu.html.twig' %}  <div class="mobile_header_menu nologin">    <ul>      <li class="logo"><a href="#" class="site_logo">ProofPilot</a></li>      <li class="right"><a href="#" class="trigger_sitemenu"></a></li>    </ul>  </div>  <div class="mobile_sitemenu register nologin">    <div class="menu_wrapper">       <a href="#" class="close"></a>       <a href="#" class="logo">ProofPilot</a>      <ul>        <li><a class="icon_register" href="#"><span>Register</span></a></li>        <li><a class="icon_login" href="#"><span>Login</span></a></li>      </ul>    </div>  </div>  <div class="intro_page">    <div class="home_welcome">      <div class="welcome_banner"><img src="{{ studycontent.studyLogo }}" alt="" height="126" width="126"></div>      <div class="welcome_text">        <h2>{{ studycontent.studyName }}</h2>        <p>{{ study.list }}</p>      </div>    </div>    <div class="mobile desc">      <p>Manilla Consulting, Emory University School of Public Health and US Centers for Disease Control</p>    </div>    <div class="choose_action">      <h3>{% trans from "study" %}txt_to_do{% endtrans %}</h3>      <ul>        <li><a class="sign_up" href="{{ path('_survey') }}"><span>{% trans from "study" %}btn_start_enrollment{% endtrans %}</span></a></li>        <li><a class="login" href="{{ path('_login') }}"><span>{% trans from "study" %}btn_log_in{% endtrans %}</span></a></li>      </ul>    </div>    <div class="text_wrapper">      <div class="video_wrapper">        <div class="video_placeholder"><img src="{{ studycontent.studyGraphic }}" height="393" width="670"></div>        <div class="video_text">          <span>{{ study.subtitle }}</span>          <p>{{ study.description }}</p>          <p><br></p>          <p>{{ study.enroll }}<br></p>        </div>      </div>      <div class="v_toolbar for_mobile">        <ul class="social">          <li><img src="images/tmp_fb.jpg" height="20" width="75"></li>          <li><img src="images/tmp_tw.jpg" height="20" width="81"></li>        </ul>        <a class="v_embed" href="#"><span><em>Embed video</em></span></a>      </div>      <div class="page_bg">        <div class="home_text">            <h2><br></h2>            {{ studycontent.studyAbout |raw }}            <div class="social_bar"><br></div>        </div>        <div class="social_bar for_mobile">          <ul class="follow_us">            <li class="text">Follow Us</li>            <li><a class="fb" href="#"><span></span></a></li>            <li><a class="tw" href="#"><span></span></a></li>          </ul>          <ul class="recommend">            <li class="text"><span class="long">Recommend to your friends</span><span class="short">Share</span></li>            <li><a class="share" href="#"><span><em>Share</em></span></a></li>          </ul>        </div>        <aside>          <div class="box">            <h3>{% trans from "study" %}txt_secure_title{% endtrans %}</h3>            {% trans from "study" %}txt_secure_text{% endtrans %}          </div>          <div class="box last">            <h3>{% trans from "study" %}txt_about_proofpilot_title{% endtrans %}</h3>            {% trans from "study" %}txt_about_proofpilot_text{% endtrans %}          </div>        </aside>      </div>    </div>  </div>{% endblock content %}