{%  extends '::base.html.twig' %}{% block body %}  <body class="has_image">    <div class="reg_header"> <a href="#" class="logo">ProofPilot</a> </div>    <div class="mobile_reg">        <h1>Enter code you received via SMS </h1>        <br>        <form method="POST" action="">            <ul class="form_fields">                <li>{{ form_widget(form.user_sms, {'attr': {'placeholder':'SMS Code', 'autocomplete': 'off', 'class': 'default_input full'}}) }}</li>            </ul>            <div class="center_align"><a href="main.html" class="submit" id="btn_confirm_code">Confirm                code &amp; login</a></div>            <div class="center_align"><a href="{{ path('_do_login') }}" class="repeat">No code? Send                another</a></div>            <button type="submit" style="display:none" id="submit_button"></button>            {{ form_rest(form) }}       </form>      </form>    </div>    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>    <script>window.jQuery || document.write('<script src= {{ asset('js/vendor/jquery-1.9.1.min.js') }}><\/script>')</script>    <script src={{ asset('js/plugins.js') }}></script>    <script src={{ asset('js/main.js') }}></script>    <script type="text/javascript">        $(document).ready(function(){            $('#btn_confirm_code').click(function()                {                        $('#submit_button').trigger('click');                        return false;                });           });    </script>   </body>{% endblock body %}