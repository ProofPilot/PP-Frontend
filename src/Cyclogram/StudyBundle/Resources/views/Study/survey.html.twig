		<script>
     		$('#iframe1').load(function(){
     		    location.href='#';
     			var frame = $(this);
         		var frameSize = function() {
         			var frameHeight = frame.contents().find("html").height();
             		frame.height(frameHeight);
                }

         	    frameSize();
         		
         		$(window).resize(frameSize);
         		frame.contents().mouseover(frameSize);

     			var surveydata = frame.contents().find('#surveydata');
		    	var surveyId = surveydata.attr('surveyid');
		    	var saveId = surveydata.attr('saveid');
		    	//var redirectUrl = '{{ path('_study',{studyId: studyId, studyUrl: studyUrl}) }}';
		    	if(surveyId !== undefined && saveId !== undefined) {
		    	    window.location = '/' + '{{ app.request.locale }}' + '/surveyResponse?&surveyId=' + surveyId + '&saveId=' + saveId + '&redirectUrl=' + '{{ app.request.get('redirectUrl') }}';
		    	}

    		});

		</script>

        
