{% extends '::general.html.twig' %}{% block header %}    {% include '::header.html.twig' %}{% endblock header %}{% block content %}  <div class="intro_page">    <div class="home_welcome">      <div class="welcome_banner"><img src="{{ logo }}" alt="" height="126" width="126"></div>      <div class="welcome_text">        <h2>{{ studycontent.studyName }}</h2>        <p>{{ studycontent.studyTagline | raw}}</p>      </div>    </div>    <div class="mobile desc">      <p>{{ studycontent.studyTagline | raw}}</p>    </div>    <div class="choose_action">      <h3>{% trans from "study" %}txt_to_do{% endtrans %}</h3>      <ul>        {% set redirectUrl = path('_study',{studyUrl: studyUrl})|url_encode  %}        {% if not is_enrolled_in_study(studyId) %}            {% if studyId == 12 %}                <li><a class="sign_up" href="{{ path('_survey', {'studyUrl': studyUrl, 'studyId': studyId, 'surveyId': surveyId} ) }}"><span>{% trans from "study" %}btn_start_enrollment{% endtrans %}</span></a></li>            {% else %}                <li><a class="sign_up" href="{{ path('_survey', {'studyId': studyId, 'surveyId': surveyId, 'redirectUrl': redirectUrl} ) }}"><span>{% trans from "study" %}btn_start_enrollment{% endtrans %}</span></a></li>            {% endif %}        {% endif %}        <li><a class="login" href="{{ path('_login') }}"><span>{% trans from "study" %}btn_log_in{% endtrans %}</span></a></li>      </ul>    </div>    {% set queryParams = app.request.query.all %}    {% if queryParams["eligible"] is defined %}    <p class="main_notification">        <span>You are not eligible for {{ studycontent.studyName }} at this time.</span>    </p>    {% endif %}        <div class="text_wrapper">      <div class="video_wrapper">        <div class="video_placeholder"><img src="{{ graphic }}" height="393" width="670"></div>        <div class="video_text">        {{ studycontent.studyTagline | raw }}        {{ studycontent.studyDescription    | raw }}        </div>      </div>      <div class="v_toolbar for_mobile">        <ul class="social">          <li><img src={{ asset ('images/tmp_fb.jpg') }} height="20" width="75"></li>          <li><img src={{ asset ('images/tmp_tw.jpg') }}  height="20" width="81"></li>        </ul>        <a class="v_embed" href="#"><span><em>Embed video</em></span></a>      </div>      <div class="page_bg">        <div class="home_text">            <h2><br></h2>            {{ studycontent.studyAbout |raw }}            {{ studycontent.studyWhatsInvolved | raw }}            {{ studycontent.studyRequirements | raw }}            <div class="social_bar"><br></div>        </div>        <div class="social_bar for_mobile">          <ul class="follow_us">            <li class="text">Follow Us</li>            <li><a class="fb" href="#"><span></span></a></li>            <li><a class="tw" href="#"><span></span></a></li>          </ul>          <ul class="recommend">            <li class="text"><span class="long">Recommend to your friends</span><span class="short">Share</span></li>            <li><a class="share" href="#"><span><em>Share</em></span></a></li>          </ul>        </div>        <aside>          <div class="box">            <h3>{% trans from "study" %}txt_secure_title{% endtrans %}</h3>            {% trans from "study" %}txt_secure_text{% endtrans %}            <p><a href="{{ path('_secure', {'studyUrl' : studyUrl, 'studyId':studyId}) }}">{% trans from "study" %}txt_security_privacy{% endtrans %}<br></a></p>            <p><br></p>          </div>          <div class="box last">            <h3>{% trans from "study" %}txt_about_proofpilot_title{% endtrans %}</h3>            {% trans from "study" %}txt_about_proofpilot_text{% endtrans %}          </div>        </aside>      </div>    </div>  </div>{% endblock content %}