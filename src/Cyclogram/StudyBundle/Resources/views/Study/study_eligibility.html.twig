{% extends '::general.html.twig' %}{% block header %}    {% include '::header.html.twig' with {'studyId' : studyId}%}{% endblock header %}{% block content %}  <div class="panel alt_bg">    <div class="panel_left study">      <br/>      <div class="menu_cont">        <ul class="left_menu">          <li><a href="#" class="icon_dashboard active">Dashboard <span class="notice">5</span></a></li>          <li class="first"><a class="icon_survey" href="#">Survey</a></li>          <li><a class="icon_activities" href="#">Activities</a></li>          <li><a class="icon_measurments" href="#">Measurments</a></li>          <li><a class="icon_treatment" href="#">Treatment</a></li>        </ul>      </div>      <div class="study_desc">      {{ studycontent.studyAbout | raw }}{#         <h3>{% trans from "study" %}txt_about{% endtrans %}</h3>#}{#           <span style="color: #7e7e7e;">{{ info }}</span>#}      </div>    </div>    <div class="panel_right">      <div class="dashboard_title green">        <h1>{% trans from "study" %}txt_you_are_eligible{% endtrans %}</h1>        <p>{% trans from "study" %}txt_eligible_dsc{% endtrans %}</p>      </div>      <div class="dashboard_panel wider">        <ul class="study_info">          <li class="image"> <img src="{{ logo }}" alt="" title="" height="103" width="103"></li>          <li class="text">            <h1>{{ studycontent.studyName }}</h1>          </li>        </ul>        <div class="mobile s_desc">          <p>Surveys allow you to provide feedback, input information all online.</p>        </div>        <div class="mobile about_box"> <a href="#" class="expander" id="about_expander"><span>{% trans from "study" %}txt_about{% endtrans %}</span></a>          <div class="text_desc">            {{ studycontent.studyConsentIntroduction }}          </div>        </div>        <p class="notice gray"><span>{% trans from "study" %}warning_message{% endtrans %}</span></p>        <div class="img_placeholder"><img src="{{ graphic }}" alt="" title=""></div>        {{ studycontent.studyConsentIntroduction | raw }}        {{ studycontent.studyConsent | raw}}        <p><br/></p>      </div>      <div class="btns_box for_mobile inlined">        <ul>          <li class="red"><a href="{{ path('_page', {'studyUrl' : studyUrl, 'eligible' : "eligible"}) }}" class="red left"><span>{% trans from "study" %}txt_decline_btn{% endtrans %}</span></a></li>          <li class="green"><a href="{{ path('_registration', {'studyId': studyId}) }}" class="green"><span>{% trans from "study" %}txt_accept_btn{% endtrans %}</span></a></li>        </ul>      </div>    </div>  </div>{% endblock content %}