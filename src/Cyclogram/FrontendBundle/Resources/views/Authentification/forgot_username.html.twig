{%  extends '::base.html.twig' %}{% form_theme form _self %}{# form_errors.html.twig #}{% block form_errors %}    {% spaceless %}        {% if errors|length > 0 %}            {% for error in errors %}                <p class="error">{{ error.message }}</p>            {% endfor %}        {% endif %}    {% endspaceless %}{% endblock form_errors %}{% block content %}    <div class="mobile_reg align_center">      <h1 class="stronger"><span>{% trans from "login" %}forgot_username{% endtrans %}?</span></h1>      <p>{% trans from "login" %}forgot_username_message{% endtrans %}</p>      <p class="align_left">{% trans from "login" %}enter_email_address{% endtrans %}</p>             {% if error is defined %}            <p class="error"><span>{{ error }}</span></p>        {% endif %}      <form method="post" action="{{ path('_forgot_username') }}">        <ul class="form_fields">          <li class="small_input">              {{ form_widget(form.participantEmail,  {'attr' : {'class' : 'text', 'id' : 'reg_field_1'}}) }}              {{ form_errors(form.participantEmail) }}          </li>        </ul>        <div class="center_align bottom_margin">        {{ form_widget(form.send,{'attr':{'class':'submit type2'}}) }}{#               {{ form_rest(form) }}#}      </form>    </div>    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>    <script>window.jQuery || document.write('<script src={{ asset('js/vendor/jquery-1.9.1.min.js') }}><\/script>')</script>        <script src= {{ asset ('js/plugins.js') }}></script>        <script src= {{ asset ('js/main.js') }}></script>        <script src= {{ asset ('js/jquery.numeric.js') }}></script>        <script>        $(document).ready(function(){            $("#phone_phone_small").numeric();            $('#phone_phone_wide').numeric();            var check = $("#phone_phone_small").val();            if (check != '') {                 $('#phone_phone_wide').focus();            }           });        </script>{% endblock content %}