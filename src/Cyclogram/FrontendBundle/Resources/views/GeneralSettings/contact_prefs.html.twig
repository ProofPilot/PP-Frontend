{%  extends '::base.html.twig' %}{% block header %}    {% include '::header.html.twig' %}{% endblock header %}    {% block body %}  <body class="page_main">    <div class="site_container">        {% include '::header.html.twig' %}      <div class="panel">        <div class="panel_left">          {% include '::profile_info.html.twig' %}          <div class="menu_cont">          {{ knp_menu_render('CyclogramFrontendBundle:MenuBuilder:createSideSettingsMenu',{'template': 'CyclogramFrontendBundle:Knp:knp_side_menu.html.twig', 'currentClass': 'active'}) }}{#             <ul class="left_menu">#}{#               <li><a class="icon_general_settings" href="general_settings.html">General#}
{#                   settings</a></li>#}{#               <li><a class="icon_contact_prefs active" href="#">Contact#}{#                   preferences</a></li>#}{#               <li><a class="icon_shipping_info" href="#">Shipping information</a></li>#}{#             </ul>#}          </div>        </div>        <div class="panel_right">          <div class="dashboard_title contact_prefs"> <span class="align_to_middle">              <h1>Contact preferences</h1>              <p>Identify how and when you want us to communicate with you.</p>            </span> </div>          <div class="dashboard_panel">            <p class="notice green nomargin margin_top"><span>Your contact                preferences have been saved</span></p>            <ul class="contact_current_data">              <li><b>My e-mail:</b> matt@cyclogram.com <a href="#">Change</a></li>              <li><b>My mobile number:</b> +1 234-567-8910 <a href="#">Change</a></li>            </ul>            <form action="" method="POST">              {% for preference in preferences %}              <div class="prefs_wrapper">                <h2>{{ preference.title }}</h2>                <a href="#" class="expand" data-text-closed="Expand" data-text-expanded="Close">Expand</a>                <div class="form_container">                  <div class="short_desc">                    <p>{{ preference.subtitle }}</p>                  </div>                  {% if preference.timezone is defined %}                      <div class="timezone_selector">                        <div class="for_label">Your timezone is:</div>                        <div class="selector">                          <div class="s_right">                            <select name="">                              <option value="">CE GMT+1</option>                            </select>                          </div>                        </div>                      </div>                  {% endif %}                  <div class="form_header">                    <ul class="{{ preference.col }}">                      <li class="select_all"><a href="#" class="btn_select_all">Select
                          All</a> / <a href="#" class="btn_select_none">Select                          None</a></li>                      {% if preference.check_here is defined %}                      <li>Check Here</li>                      {% endif %}                      {% if preference.sms_email is defined %}                      <li>SMS</li>                      <li>E-mail</li>                      {% endif %}                    </ul>                  </div>                  {% for item in preference.options %}                  <div class="form_row">                    <ul class="{{ preference.col }}">                      <li class="form_label">                        <p>{{ item.row }}</p>                      </li>                      {% if preference.check_here is defined %}                      <li><label><input name="" value="" type="checkbox"><span></span></label></li>                      {% endif %}                      {% if preference.sms_email is defined %}                      <li><label><input name="" value="" checked="checked" type="checkbox"><span>SMS</span></label></li>                      <li><label><input name="" value="" checked="checked" type="checkbox"><span>E-mail</span></label></li>                      {% endif %}                    </ul>                  </div>                  {% endfor %}                  <a href="#" class="btn save_updates"><span>Save updates</span></a>                </div>              </div>              {% endfor %}            </form>          </div>        </div>      </div>    </div>    <footer>      <div class="site_container">        <div class="footer_col_left">          <ul>            <li class="icon_home"><a class="icon_home" href="#"><span>Home</span></a></li>            <li class="icon_fullscreen"><a class="icon_fullscreen" href="#"><span>Fullscreen</span></a></li>            <li class="icon_update"><a class="icon_update" href="#"><span>Update</span></a></li>          </ul>        </div>        <div class="footer_col_middle"><a class="to_top" href="#"><span>ON TOP</span></a></div>        <div class="footer_col_right">          <ul>            <li class="icon_help"><a class="icon_help" href="#"><span>Help</span></a></li>            <li class="icon_logout"><a class="icon_logout" href="#"><span>Logout</span></a></li>            <li class="icon_settings"><a class="icon_settings" href="#"><span>My                  Settings</span></a></li>          </ul>        </div>      </div>      <a href="http://www.kudos.hr" target="_blank" class="author">design by kudos studio</a> </footer>    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>  </body>    {% endblock body %}