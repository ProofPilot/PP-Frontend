{%  extends '::dashboard.html.twig' %}{% from '::macro.html.twig' import renderexpandbutton,showexpanded %}{% block header %}    {% include '::header.html.twig' %}{% endblock header %}    {% block content %}    <div class="site_container">      {% include '::header.html.twig' %}      <div class="panel">        <div class="panel_left">{#           {% include '::profile_info.html.twig' %}#}          <div class="menu_cont">          {{ knp_menu_render('CyclogramFrontendBundle:MenuBuilder:createSideSettingsMenu',{'template': 'CyclogramFrontendBundle:Knp:knp_side_menu.html.twig', 'currentClass': 'active'}) }}          </div>        </div>        <div class="panel_right">          <div class="dashboard_title contact_prefs"> <span class="align_to_middle">              <h1>Contact preferences</h1>              <p>Identify how and when you want us to communicate with you.</p>            </span> </div>          <div class="dashboard_panel">            {%  if message is defined %}                <p class="notice green nomargin margin_top"><span>Your contact                    preferences have been saved</span></p>            {%  endif %}            <ul class="contact_current_data">               <li><b>{% trans from "contact_preferences" %}txt_my_email{% endtrans %}</b>&nbsp;{{ participant.participantEmail }}{#<a href="#">Change</a>#}</li>              <li><b>{% trans from "contact_preferences" %}txt_my_mobile_form{% endtrans %}</b>&nbsp;{{ participant.participantMobileNumber }}{#<a href="#">Change</a>#}</li>            </ul>            <form id="contact_prefs_form" action="{{ path('_contact_prefs') }}" method="POST">              <div class="prefs_wrapper">                <h2>{% trans from "contact_preferences" %}txt_how_to_receive_study_reminders{% endtrans %}</h2>                {{ renderexpandbutton('expanded', expanded) }}                <div class="form_container" {{ showexpanded('expanded', expanded) }}>                  <div class="short_desc">                    <p>{% trans from "contact_preferences" %}txt_how_would_you_like_communicate{% endtrans %}</p>                  </div>                  <div class="form_header">                    <ul class="col_5">                      <li class="select_all"><a href="#" class="btn_select_all">{% trans from "contact_preferences" %}btn_select_all{% endtrans %}                      </a> / <a href="#" class="btn_select_none">{% trans from "contact_preferences" %}btn_select_none{% endtrans %}</a></li>                      <li>SMS</li>                      <li>E-mail</li>                    </ul>                  </div>                  {% for item in remindersData %}                  <div class="form_row">                    <ul class="col_5">                      <li class="form_label">                        <p>{{ item.reminder.participantStudyReminderName|trans({},'contact_preferences') }}</p>                      </li>                      <li><label><input {% if item.bySMS %}checked="checked"{% endif %} name="sms_{{ item.reminder.participantStudyReminderId }}" id="sms_{{ item.reminder.participantStudyReminderId }}" value="{{ item.reminder.participantStudyReminderId }}" type="checkbox"><span>SMS</span></label></li>                      <li><label><input {% if item.byEmail %}checked="checked"{% endif %} name="email_{{ item.reminder.participantStudyReminderId }}" id="email_{{ item.reminder.participantStudyReminderId }}" value="{{ item.reminder.participantStudyReminderId }}" type="checkbox"><span>E-mail</span></label></li>                    </ul>                  </div>                  {% endfor %}                </div>              </div>                                          <div class="prefs_wrapper">                <h2>{% trans from "contact_preferences" %}txt_what_time_of_day_suits{% endtrans %}</h2>                 {{ renderexpandbutton('expanded', expanded) }}                <div class="form_container" {{ showexpanded('expanded', expanded) }}>                  <div class="short_desc">                    <p>{% trans from "contact_preferences" %}txt_tell_us_convenient_time{% endtrans %}</p>                  </div>                  <div class="timezone_selector">                    <div class="for_label">{% trans from "contact_preferences" %}txt_your_timezone_is{% endtrans %}</div>                    <div class="selector">                      <div class="s_right">                        <select name="timezone">                          {% for item in timezones %}                              <option value="{{ item.participantTimezoneId }}" {% if timezone is defined and timezone == item.participantTimezoneId %}selected{% endif %}>{{ item.participantTimezoneDesc|trans({},'contact_preferences') }}</option>                          {% endfor %}                        </select>                      </div>                    </div>                  </div>                                    <div class="form_header">                    <ul class="col_4">                      <li class="select_all"><a href="#" class="btn_select_all">{% trans from "contact_preferences" %}btn_select_all{% endtrans %}                      </a> / <a href="#" class="btn_select_none">{% trans from "contact_preferences" %}btn_select_none{% endtrans %}</a></li>                      <li>{% trans from "contact_preferences" %}txt_check_here{% endtrans %}</li>                    </ul>                  </div>                  {% for item in contactTimesData %}                  <div class="form_row">                    <ul class="col_4">                      <li class="form_label">                        <p>{{ item.contactTime.participantContactTimesName|trans({},'contact_preferences') }}</p>                      </li>                      <li><label><input name="contactTime_{{ item.contactTime.participantContactTimesId }}" class="contactTime"{% if item.active == true %}checked="checked"{% endif %} value="{{ item.contactTime.participantContactTimesId }}" type="checkbox" ><span>{% trans from "contact_preferences" %}txt_check_here{% endtrans %}</span></label></li>                    </ul>                  </div>                  {% endfor %}                </div>              </div>                                          <div class="prefs_wrapper">                <h2>{% trans from "contact_preferences" %}txt_what_day_of_week_suits{% endtrans %}</h2>                 {{ renderexpandbutton('expanded', expanded) }}                <div class="form_container" {{ showexpanded('expanded', expanded) }}>                  <div class="short_desc">                    <p>{% trans from "contact_preferences" %}txt_how_what_day_is_best{% endtrans %}</p>                  </div>                  <div class="form_header">                    <ul class="col_4">                      <li class="select_all"><a href="#" class="btn_select_all">{% trans from "contact_preferences" %}btn_select_all{% endtrans %}                      </a> / <a href="#" class="btn_select_none">{% trans from "contact_preferences" %}btn_select_none{% endtrans %}</a></li>                      <li>{% trans from "contact_preferences" %}txt_check_here{% endtrans %}</li>                    </ul>                  </div>                  {%  for item in weekDaysData %}                  <div class="form_row">                    <ul class="col_4">                      <li class="form_label">                        <p>{{ item.dayname|trans({},'contact_preferences') }}</p>                      </li>                      <li><label><input name="weekday_{{ item.dayid }}" class="weekDays" {% if item.active == true %}checked="checked"{% endif %} value="day{{ item.dayid }}" type="checkbox"><span>{% trans from "contact_preferences" %}txt_check_here{% endtrans %}</span></label></li>                    </ul>                  </div>                  {% endfor %}                </div>              </div>              <a href="#" class="btn save_updates" id="save_contact_prefs"><span>{% trans from  "contact_preferences" %}btn_save_updates{% endtrans %}</span></a>              <input type="hidden" value="" name="savedata" id="savedata">            </form>          </div>        </div>      </div>    </div>        {% include '::footer.html.twig' %}    {% endblock content %}