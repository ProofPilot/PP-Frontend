{%  extends '::dashboard.html.twig' %}{% from '::macro.html.twig' import renderexpandbutton,showexpanded %}{% block header %}    {% include '::header.html.twig' %}{% endblock header %}    {% block content %}    <div class="site_container">      {% include '::header.html.twig' %}      <div class="panel">        <div class="panel_left">          {% include '::profile_info.html.twig' %}          <div class="menu_cont">          {{ knp_menu_render('CyclogramFrontendBundle:MenuBuilder:createSideSettingsMenu',{'template': 'CyclogramFrontendBundle:Knp:knp_side_menu.html.twig', 'currentClass': 'active'}) }}          </div>        </div>        <div class="panel_right">          <div class="dashboard_title contact_prefs"> <span class="align_to_middle">              <h1>Contact preferences</h1>              <p>Identify how and when you want us to communicate with you.</p>            </span> </div>          <div class="dashboard_panel">            {%  if message is defined %}                <p class="notice green nomargin margin_top"><span>Your contact                    preferences have been saved</span></p>            {%  endif %}            <ul class="contact_current_data">               <li><b>{% trans from "contact_preferences" %}txt_my_email{% endtrans %}:</b>&nbsp;{{ participant.participantEmail }}{#<a href="#">Change</a>#}</li>              <li><b>{% trans from "contact_preferences" %}txt_my_mobile_form{% endtrans %}:</b>&nbsp;{{ participant.participantMobileNumber }}{#<a href="#">Change</a>#}</li>            </ul>            <form id="contact_prefs_form" action="{{ path('_contact_prefs') }}" method="POST">              <div class="prefs_wrapper">                <h2>{% trans from "contact_preferences" %}txt_how_to_receive_study_reminders{% endtrans %}</h2>                {{ renderexpandbutton('reminders', expandedForm) }}                <div class="form_container" {{ showexpanded('reminders', expandedForm) }}>                  <div class="short_desc">                    <p>{% trans from "contact_preferences" %}txt_how_would_you_like_communicate{% endtrans %}</p>                  </div>                  <div class="form_header">                    <ul class="col_5">                      <li class="select_all"><a href="#" class="btn_select_all">{% trans from "contact_preferences" %}btn_select_all{% endtrans %}                      </a> / <a href="#" class="btn_select_none">{% trans from "contact_preferences" %}btn_select_none{% endtrans %}</a></li>                      <li>SMS</li>                      <li>E-mail</li>                    </ul>                  </div>                  {% for item in remindersData %}                  <div class="form_row">                    <ul class="col_5">                      <li class="form_label">                        <p>{{ item.reminder.participantStudyReminderName|trans({},'contact_preferences') }}</p>                      </li>                      <li><label><input {% if item.bySMS %}checked="checked"{% endif %} name="sms_{{ item.reminder.participantStudyReminderId }}" value="{{ item.reminder.participantStudyReminderId }}" type="checkbox"><span>SMS</span></label></li>                      <li><label><input {% if item.byEmail %}checked="checked"{% endif %} name="email_{{ item.reminder.participantStudyReminderId }}" value="{{ item.reminder.participantStudyReminderId }}" type="checkbox"><span>E-mail</span></label></li>                    </ul>                  </div>                  {% endfor %}                  <a href="#" class="btn save_updates" id="save_reminders"><span>{% trans from  "contact_preferences" %}btn_save_updates{% endtrans %}</span></a>                </div>              </div>                                          <div class="prefs_wrapper">                <h2>{% trans from "contact_preferences" %}txt_what_time_of_day_suits{% endtrans %}</h2>                 {{ renderexpandbutton('contacttimes', expandedForm) }}                <div class="form_container" {{ showexpanded('contacttimes', expandedForm) }}>                  <div class="short_desc">                    <p>{% trans from "contact_preferences" %}txt_tell_us_convenient_time{% endtrans %}</p>                  </div>                  <div class="timezone_selector">                    <div class="for_label">{% trans from "contact_preferences" %}txt_your_timezone_is{% endtrans %}</div>                    <div class="selector">                      <div class="s_right">                        <select name="timezone">                          {% for item in timezones %}                              <option value="{{ item.participantTimezoneId }}" {% if timezone is defined and timezone == item.participantTimezoneId %}selected{% endif %}>{{ item.participantTimezoneDesc|trans({},'contact_preferences') }}</option>                          {% endfor %}                        </select>                      </div>                    </div>                  </div>                                    <div class="form_header">                    <ul class="col_4">                      <li class="select_all"><a href="#" class="btn_select_all">{% trans from "contact_preferences" %}btn_select_all{% endtrans %}                      </a> / <a href="#" class="btn_select_none">{% trans from "contact_preferences" %}btn_select_none{% endtrans %}</a></li>                      <li>{% trans from "contact_preferences" %}txt_check_here{% endtrans %}</li>                    </ul>                  </div>                  {% for item in contactTimesData %}                  <div class="form_row">                    <ul class="col_4">                      <li class="form_label">                        <p>{{ item.contactTime.participantContactTimesName|trans({},'contact_preferences') }}</p>                      </li>                      <li><label><input name="contactTime_{{ item.contactTime.participantContactTimesId }}" {% if item.active == true %}checked="checked"{% endif %} value="{{ item.contactTime.participantContactTimesId }}" type="checkbox"><span>{% trans from "contact_preferences" %}txt_check_here{% endtrans %}</span></label></li>                    </ul>                  </div>                  {% endfor %}                  <a href="#" class="btn save_updates" id="save_time_of_day"><span>Save updates</span></a>                </div>              </div>                                          <div class="prefs_wrapper">                <h2>{% trans from "contact_preferences" %}txt_what_day_of_week_suits{% endtrans %}</h2>                 {{ renderexpandbutton('contactweekdays', expandedForm) }}                <div class="form_container" {{ showexpanded('contactweekdays', expandedForm) }}>                  <div class="short_desc">                    <p>{% trans from "contact_preferences" %}txt_how_what_day_is_best{% endtrans %}</p>                  </div>                  <div class="form_header">                    <ul class="col_4">                      <li class="select_all"><a href="#" class="btn_select_all">{% trans from "contact_preferences" %}btn_select_all{% endtrans %}                      </a> / <a href="#" class="btn_select_none">{% trans from "contact_preferences" %}btn_select_none{% endtrans %}</a></li>                      <li>{% trans from "contact_preferences" %}txt_check_here{% endtrans %}</li>                    </ul>                  </div>                  {%  for item in weekDaysData %}                  <div class="form_row">                    <ul class="col_4">                      <li class="form_label">                        <p>{{ item.dayname|trans({},'contact_preferences') }}</p>                      </li>                      <li><label><input name="weekday_{{ item.dayid }}" {% if item.active == true %}checked="checked"{% endif %} value="{{ item.dayid }}" type="checkbox"><span>{% trans from "contact_preferences" %}txt_check_here{% endtrans %}</span></label></li>                    </ul>                  </div>                  {% endfor %}                  <a href="#" class="btn save_updates" id="save_day_of_week"><span>Save updates</span></a>                </div>              </div>              <input type="hidden" value="" name="savedata" id="savedata">            </form>          </div>        </div>      </div>    </div>    <footer>      <div class="site_container">        <div class="footer_col_left">          <ul>            <li class="icon_home"><a class="icon_home" href="#"><span>Home</span></a></li>            <li class="icon_fullscreen"><a class="icon_fullscreen" href="#"><span>Fullscreen</span></a></li>            <li class="icon_update"><a class="icon_update" href="#"><span>Update</span></a></li>          </ul>        </div>        <div class="footer_col_middle"><a class="to_top" href="#"><span>ON TOP</span></a></div>        <div class="footer_col_right">          <ul>            <li class="icon_help"><a class="icon_help" href="#"><span>Help</span></a></li>            <li class="icon_logout"><a class="icon_logout" href="#"><span>Logout</span></a></li>            <li class="icon_settings"><a class="icon_settings" href="#"><span>My                  Settings</span></a></li>          </ul>        </div>      </div>    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>    <script type="text/javascript">        $(document).ready(function(){           $('#save_reminders').click(function()                {        	            $('#savedata').val('reminders');                         $('#contact_prefs_form').trigger('submit');                });           $('#save_day_of_week').click(function()                   {        	               $('#savedata').val('date_of_week');                           $('#contact_prefs_form').trigger('submit');                   });           $('#save_time_of_day').click(function()                   {        	                $('#savedata').val('time_of_day');                            $('#contact_prefs_form').trigger('submit');                   });        });      </script>    {% endblock content %}