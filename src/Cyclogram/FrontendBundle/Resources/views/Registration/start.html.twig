{% extends '::base.html.twig' %}{% use '::facebook_blocks.html.twig' %}{% use '::google_blocks.html.twig' %}{% form_theme form '::fields_theme1.html.twig' %}{% block javascripts %}<script src= {{ asset ('js/jstz-1.0.4.min.js') }}></script><script>       $(document).ready(function(){           $('#facebook_button').click(function(e)                {                     e.preventDefault();                    e.stopPropagation();                    fb_login();                });           $('#google_button').click(function(e)                {                    e.preventDefault();                    e.stopPropagation();                    g_login();                });           $('#registration_next').click(function()                {                    var tz = jstz.determine();                    var timeZone = tz.name();                    $('#registration_timeZone').val(timeZone);                    return true;               })           var tz = jstz.determine();           $.post("{{ path('_register') }}", {timeZonename : tz.name()})       });</script>{% endblock javascripts %}{% block content %}    <div class="mobile_reg login_form">      <div class="wrapper">        <h1><span>{% trans from "register" %}txt_join{% endtrans %} 1/{{ totalSteps }}:</span> {% trans from "register" %}txt_basics{% endtrans %}</h1>        <div class="loginform_left register">          <div class="btns_wapper">               <a class="btn_reg fb" id="facebook_button" href="">                   <span>{% trans from "register" %}txt_join_with{% endtrans %} Facebook</span>               </a>               <a class="btn_reg google" id="google_button" href="">                   <span>{% trans from "register" %}txt_join_with{% endtrans %} Google</span>               </a>           </div>          <div id="why_popup" class="why_popup">              <div class="why_popup_content">                  <a class="why_close" href="#"></a>                  <p><strong>{% trans from "login" %}why_popup_title{% endtrans %}</strong></p>                  <p>{% trans from "login" %}txt_why_popup{% endtrans %}</p>              </div>          </div>          <div class="notice2">            <a id="open_why_popup"><span>{% trans from "register" %}txt_why{% endtrans %}</span></a>          </div>        </div>        <div class="sep wide"> OR </div>        <div class="loginform_right">          <form novalidate action="{{ path('_register', {'studyCode' : studyCode}) }}" method="post" name="registration" novalidate>                        <label for="reg_field_1">{% trans from "register" %}enter_email{% endtrans %}</label>               {{ form_widget(form.participantEmail, {'attr' : {'class' : 'text', 'id' : 'reg_field_1'}}) }}              {{ form_errors(form.participantEmail) }}              <label for="reg_field_2">{% trans from "register" %}pick_user{% endtrans %}</label>               {{ form_widget(form.participantUsername, {'attr' : {'class' : 'text', 'id' : 'reg_field_2'}}) }}              {{ form_errors(form.participantUsername) }}              <label for="reg_field_3">{% trans from "register" %}create_password{% endtrans %}</label>              <span class="note">{% trans from "register" %}min_charachters{% endtrans %}</span>               {{ form_widget(form.participantPassword.first, {'attr' : {'class' : 'text', 'id' : 'reg_field_3'}}) }}              {{ form_errors(form.participantPassword.first) }}                            <label for="reg_field_4">{% trans from "register" %}confirm_password{% endtrans %}</label>              {{ form_widget(form.participantPassword.second, {'attr' : {'class' : 'text', 'id' : 'reg_field_4'}}) }}                              <div class="actions_row">              {{form_widget(form.next, {'attr':{'class':'submit with_min_width btn_login'}}) }}              </div>              {{ form_rest(form) }}              {{ form_errors(form) }}          </form>          <div class="mobile_text">Allready Joined? <a href="#">Log in</a></div>        </div>      </div>    </div>    <div class="login_btn_placeholder">{% trans from "register" %}already_joined{% endtrans %}<a href="{{ path('_login') }}">Log in</a></div>  </body>{% endblock content %}