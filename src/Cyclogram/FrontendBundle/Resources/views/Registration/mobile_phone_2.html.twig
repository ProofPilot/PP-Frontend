{%  extends '::base.html.twig' %}{% block body %}  <body class="has_image">    <div class="reg_header"> <a href="#" class="logo">ProofPilot</a> </div>    <div class="mobile_reg">      <h1><span>JOIN 3/4:</span> Double Check You Entry</h1>      <ul class="confirm_list">        <li><span>USA</span> {{ phone }}</li>      </ul>      <div class="center_align m">         <a href={{ path('reg_step_2') }} class="submit m1 gray">Wrong.Go back.</a>         <a href={{ path('reg_step_3') }} class="submit m2">Ok.Verify!</a>      </div>    </div>    <script>        $(document).ready(function(){            $('#register_submit').click(function() {                $('#submit_button').trigger('click');                return false;            });        });    </script>  </body>{% endblock body %}