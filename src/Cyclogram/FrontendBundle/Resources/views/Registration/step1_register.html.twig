{%  extends '::base.html.twig' %}{% form_theme form _self %}{# form_errors.html.twig #}{% block form_errors %}    {% spaceless %}        {% if errors|length > 0 %}            {% for error in errors %}                <p class="error">{{ error.message }}</p>            {% endfor %}        {% endif %}    {% endspaceless %}{% endblock form_errors %}{% block form_widget_simple %}{% spaceless %}    {% set type = type|default('text') %}    {% if errors %}        {% set class_error = attr.class ~ " has_error" %}        {% set attr = attr|merge({'class': class_error}) %}     {% endif %}    <span class="input_placeholder {% if errors %}has_error{% endif %}">        <input type="{{ type }}" {{ block('widget_attributes') }} {% if value is not empty %} value="{{ value }}" {% endif %}/>        <span class="mark"></span>    </span>{% endspaceless %}{% endblock form_widget_simple %}{% block javascripts %}<script>       function goLogIn(){           window.location = "{{ path('_fb_security_check') }}";       }       function onFbInit() {           if (typeof(FB) != 'undefined' && FB != null ) {               FB.Event.subscribe('auth.statusChange', function(response) {                   setTimeout(goLogIn, 500);               });           }           FB.Event.subscribe('auth.logout',                    function(response) {                        if (response.status === 'unknown')  {                           window.location = "{{ path('_logout') }}";                         }           });                                   }</script>{% endblock javascripts %}{% block body %}<body class="has_image">  {{ facebook_initialize({'xfbml': true, 'fbAsyncInit': 'onFbInit();'}) }}    {% include '::header_reg.html.twig' %}    <div class="mobile_reg login_form">      <div class="wrapper">        <h1><span>{% trans from "register" %}txt_join{% endtrans %} 1/6:</span> The Basics</h1>        <div class="loginform_left register">          <div class="btns_wapper">            <form>             {{ facebook_login_button({'autologoutlink': true}, 'CyclogramFrontendBundle:Facebook:joinButton.html.twig') }}{#             <a class="btn_reg fb" href="#"><span>Join with Facebook</span></a>#}            </form>            <form> <a id="expand_google_form" class="btn_reg google" href="#"><span>Join                  with Google</span></a> </form>          </div>          <div class="notice2">            <p><span>{% trans from "register" %}txt_why{% endtrans %}</span></p>          </div>        </div>        <div class="sep wide"> OR </div>        <div class="loginform_right">          <form novalidate action="{{ path('_registration') }}" method="post" name="registration">                         <label for="reg_field_1">{% trans from "register" %}enter_email{% endtrans %}</label>               {{ form_widget(form.participantEmail, {'attr' : {'class' : 'text', 'id' : 'reg_field_1'}}) }}              {{ form_errors(form.participantEmail) }}              <label for="reg_field_2">{% trans from "register" %}pick_user{% endtrans %}</label>               {{ form_widget(form.participantUsername, {'attr' : {'class' : 'text', 'id' : 'reg_field_2'}}) }}              {{ form_errors(form.participantUsername) }}              <label for="reg_field_3">{% trans from "register" %}create_password{% endtrans %}</label>              <span class="note">{% trans from "register" %}min_charachters{% endtrans %}</span>               {{ form_widget(form.participantPassword.first, {'attr' : {'class' : 'text', 'id' : 'reg_field_3'}}) }}              {{ form_errors(form.participantPassword.first) }}                            <label for="reg_field_4">{% trans from "register" %}confirm_password{% endtrans %}</label>              {{ form_widget(form.participantPassword.second, {'attr' : {'class' : 'text', 'id' : 'reg_field_4'}}) }}                            {{ form_rest(form) }}              {{ form_errors(form) }}                              <div class="actions_row"> <a href="#" class="submit with_min_width btn_login" id="register_submit">NEXT&gt;</a>               <button type="submit" style="display:none" id="submit_button"></button></div>                        </form>          <div class="mobile_text">Allready Joined? <a href="#">Log in</a></div>        </div>      </div>    </div>    <div class="login_btn_placeholder">{% trans from "register" %}already_joined{% endtrans %}<a href="{{ path('_login') }}">Log in</a>    {% include '::footer_reg.html.twig' %}    </div>    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>    <script>window.jQuery || document.write('<script src={{ asset('js/vendor/jquery-1.9.1.min.js') }}><\/script>')</script>        <script src= {{ asset ('js/plugins.js') }}></script>        <script src= {{ asset ('js/main.js') }}></script>               <script type="text/javascript">        $(document).ready(function(){           $('#register_submit').click(function()                {                         $('#submit_button').trigger('click');                        return false;                });           $('#facebook_button').click(function()                    {                             FB.login(null, {scope: 'email, user_hometown, user_location'});                    });           });           $('.mark').click(function() {                $(this).css('display', 'none').siblings().removeClass('has_error').val('').parent().removeClass('has_error');                $('.error').css('display', 'none');           });        </script>   </body>{% endblock body %}