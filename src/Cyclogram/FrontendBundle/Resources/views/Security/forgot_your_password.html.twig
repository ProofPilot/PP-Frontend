{%  extends '::base.html.twig' %}{% block body %}  <body class="has_image">    <div class="reg_header"> <a href="page.html" class="logo">ProofPilot</a> </div>    <div class="mobile_reg align_center">      <h1 class="stronger smaller_margin"><span>Forgot your password?</span></h1>      <p>It happens to all of us more times that we'd like to admin. No problem,        we'll walk you through creating a new password while protecting your        privacy and security. </p>        {% if error is defined %}            <p class="error"><span>{{ error }}</span></p>        {% endif %}      <form method="post" action="{{ path('_forgot_pass') }}">        <ul class="form_fields small_margin">          <li class="text_left short">Username</li>          <li class="input_right wide">          {{ form_widget(form.participantUsername, {'attr' : {'class' : 'username_input'}}) }}          {{ form_errors(form.participantUsername) }}          </li>              {{ form_rest(form) }}              {{ form_errors(form) }}        </ul>        <div class="center_align bottom_margin">            <a href="" class="submit" id="reset_pass">{% trans from "login" %}passsword_reset_email{% endtrans %}</a>        </div>        <div class="center_align bottom_margin">        <a href="{{ path('_forgot_username') }}" class="cant_remember">            <span>Canâ€™t remember your username?</span>        </a>        <button type="submit" style="display:none" id="submit_button"></button>        </div>      </form>    </div>    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>    <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.9.1.min.js"><\/script>')</script>    <script src="js/plugins.js"></script>    <script src="js/main.js"></script>    <script src= {{ asset ('js/main.js') }}></script>               <script type="text/javascript">        $(document).ready(function(){            $('#reset_pass').click(function()                {                         $('#submit_button').trigger('click');                        return false;                });           });        </script>   </body>{% endblock body %}