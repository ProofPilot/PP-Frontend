{%  extends '::base.html.twig' %}{% form_theme form _self %}{# form_errors.html.twig #}{% block form_errors %}    {% spaceless %}        {% if errors|length > 0 %}            {% for error in errors %}                <p class="error">{{ error.message }}</p>            {% endfor %}        {% endif %}    {% endspaceless %}{% endblock form_errors %}{% block body %}  <body class="has_image">    {% include '::header_reg.html.twig' %}    <div class="mobile_reg align_center">      <h1 class="stronger smaller_margin"><span>Confirm Password Reset</span></h1>      <p>To protect the security of your account, please enter the mobile code        we just sent to your mobile phone.</p>      <form method="post" action="{{ path('_confirm_pass_reset', {'id' : id}) }}">        <ul class="form_fields small_margin">            <li class="text_left mobile_code">Enter mobile code </li>            <li class="input_right mobile_code">             {{ form_widget(form.sms_code, {'attr' : {'class' : 'username_input'}}) }}             {{ form_errors(form.sms_code) }}                     {% if error is defined %}                         <p class="error"><span>{{ error }}</span></p>                     {% endif %}                          {{ form_rest(form) }}             {{ form_errors(form) }}{#                 <input name="" class="username_input" value="" type="text">#}            </li>        </ul>        <div class="center_align bottom_margin">            <a href="password_changed.html" class="submit" id="register_submit">CONFIRM NEW PASSWORD</a>            <button type="submit" style="display:none" id="submit_button"></button>        </div>        <div class="center_align bottom_margin">            <a href="#" class="no_code"><span>No code? Send another</span></a>        </div>      </form>    </div>    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>    <script>window.jQuery || document.write('<script src={{ asset('js/vendor/jquery-1.9.1.min.js') }}><\/script>')</script>        <script src= {{ asset ('js/plugins.js') }}></script>        <script src= {{ asset ('js/main.js') }}></script>               <script type="text/javascript">        $(document).ready(function(){            $('#register_submit').click(function()                {                         $('#submit_button').trigger('click');                        return false;                });           });        </script>   </body>{% endblock body %}