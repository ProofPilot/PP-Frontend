{%  extends '::base.html.twig' %}{% form_theme form _self %}{# form_errors.html.twig #}{% block form_errors %}    {% spaceless %}        {% if errors|length > 0 %}            {% for error in errors %}                <p class="error">{{ error.message }}</p>            {% endfor %}        {% endif %}    {% endspaceless %}{% endblock form_errors %}{% block body %}  <body class="has_image">    {% include '::header_reg.html.twig' %}    <div class="mobile_reg align_center">      <h1 class="stronger"><span>{% trans from "login" %}forgot_username{% endtrans %}?</span></h1>      <p>{% trans from "login" %}forgot_username_message{% endtrans %}</p>      <p class="align_left">{% trans from "login" %}enter_mobile_phone{% endtrans %}</p>             {% if error is defined %}            <p class="error"><span>{{ error }}</span></p>        {% endif %}      <form method="post" action="{{ path('_forgot_username') }}">        <ul class="form_fields">          <li class="small_input">              {{ form_widget(form.phone_small, {'attr' : {'class' : 'default_input tac small'}}) }}              {{ form_errors(form.phone_small) }}          </li>                    <li class="wide_input">              {{ form_widget(form.phone_wide, {'attr' : {'class' : 'default_input wide'}}) }}              {{ form_errors(form.phone_wide) }}              {{ form_errors(form) }}              {{ form_rest(form) }}          </li>        </ul>        <div class="center_align bottom_margin">        <a href="#" class="submit type2" id="register_submit">{% trans from "login" %}btn_send_username_to_mobilephone{% endtrans %}</a>            <button type="submit" style="display:none" id="submit_button"></button>            </div>      </form>    </div>    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>    <script>window.jQuery || document.write('<script src={{ asset('js/vendor/jquery-1.9.1.min.js') }}><\/script>')</script>        <script src= {{ asset ('js/plugins.js') }}></script>        <script src= {{ asset ('js/main.js') }}></script>        <script src= {{ asset ('js/jquery.numeric.js') }}></script>        <script>        $(document).ready(function(){            $('#register_submit').click(function()                {                         $('#submit_button').trigger('click');                        return false;                });            $("#phone_phone_small").numeric();            $('#phone_phone_wide').numeric();           });        </script>  </body>{% endblock body %}