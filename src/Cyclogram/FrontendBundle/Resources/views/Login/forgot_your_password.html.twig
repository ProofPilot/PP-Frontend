{%  extends '::base.html.twig' %}{% block content %}    <div class="mobile_reg align_center">      <h1 class="stronger smaller_margin"><span>{% trans from "login" %}forgot_password{% endtrans %}?</span></h1>      <p>{% trans from "login" %}forgot_password_message{% endtrans %}</p>        {% if error is defined %}            <p class="error"><span>{{ error }}</span></p>        {% endif %}      <form method="post" action="{{ path('_forgot_pass', {'studyCode':studyCode}) }}">        <ul class="form_fields small_margin">          <li class="text_left short">{% trans from "login" %}txt_form_username{% endtrans %}</li>          <li class="input_right wide">          {{ form_widget(form.participantUsername, {'attr' : {'class' : 'username_input'}}) }}          {{ form_errors(form.participantUsername) }}          </li>                            {{ form_errors(form) }}        </ul>        <div class="center_align bottom_margin">            {{ form_widget(form.sendPass, {'attr':{'class':'submit'}}) }}            {{ form_rest(form) }}        </div>        <div class="center_align bottom_margin">        <a href="{{ path('_forgot_username') }}" class="cant_remember">            <span>{% trans from "login" %}cant_remember_username{% endtrans %}</span>        </a>        <button type="submit" style="display:none" id="submit_button"></button>        </div>      </form>    </div>    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>    <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.9.1.min.js"><\/script>')</script>    <script src="js/plugins.js"></script>    <script src="js/main.js"></script>    <script src= {{ asset ('js/main.js') }}></script>{% endblock content %}