{% extends '::base.html.twig' %}{% block body %}    {% if studyId == 1 %}        <body class="knowathome{{ nPic }}">    {% else  %}        <body class="has_image">    {%  endif %}    {% include '::header_reg.html.twig' %}    <div class="mobile_reg">        <h1>{% trans from "login" %}enter_sms_code{% endtrans %}</h1>        <br>        <form method="POST" action="">            <ul class="form_fields">                <li>{{ form_widget(form.user_sms, {'attr': {'placeholder':'SMS Code', 'autocomplete': 'off', 'class': 'default_input full'}}) }}</li>            </ul>            <div class="center_align"><a href="main.html" class="submit" id="btn_confirm_code">{% trans from "login" %}btn_confrimcoe_login{% endtrans %}</a></div>            <div class="center_align"><a href="{{ path('_do_login') }}" class="repeat">{% trans from "login" %}send_another_code{% endtrans %}</a></div>            <button type="submit" style="display:none" id="submit_button"></button>            {{ form_rest(form) }}        </form>    </div>    <script type="text/javascript">        $(document).ready(function(){            $('#btn_confirm_code').click(function() {                $('#submit_button').trigger('click');                    return false;            });        });    </script>  </body>{% endblock body %}