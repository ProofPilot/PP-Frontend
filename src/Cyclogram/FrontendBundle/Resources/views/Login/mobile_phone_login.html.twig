{% extends '::base.html.twig' %}{% block body %}        <body class="has_image" {% if bgimage is defined %}style="background-image:url('{{ bgimage }}')"{% endif %}>    {% include '::header_reg.html.twig' %}    <div class="mobile_reg">        <h1>{% trans from "login" %}enter_sms_code{% endtrans %}</h1>        <br>        <form method="POST" action="{{ path('login_sms', {'studyId' : studyId}) }}">            <ul class="form_fields">                <li>{{ form_widget(form.sms_code, {'attr': {'placeholder':'SMS Code', 'autocomplete': 'off', 'class': 'default_input full'}}) }}</li>            </ul>            <div class="center_align">            {{ form_widget(form.confirmCode, {'attr':{'class':'submit'}}) }}            </div>            <div class="center_align"><a href="{{ path('_do_login') }}" class="repeat">{% trans from "login" %}send_another_code{% endtrans %}</a></div>            <button type="submit" style="display:none" id="submit_button"></button>            {{ form_rest(form) }}        </form>    </div>  </body>{% endblock body %}