{%  extends '::dashboard.html.twig' %}{% use '::facebook_blocks.html.twig' %}{% use '::google_blocks.html.twig' %}{% block javascripts %}<script type="text/javascript">      $(document).ready(function(){            $('.icon_logout').click(function()            {            	{% if is_granted('ROLE_FACEBOOK_USER') %}                  fbLogout();                {% elseif is_granted('ROLE_GOOGLE_USER') %}                  gLogout();                  {% endif %}                                  window.location=Routing.generate('_logout');            });      });</script> {% endblock javascripts %}{% block content %}        <div class="site_container">            {% include '::header.html.twig'%}                  <div class="panel">            <div class="panel_left">              {% include '::profile_info.html.twig' %}              <div class="menu_cont">              {% if studyCode != 'kah' %}                  {{ knp_menu_render('CyclogramFrontendBundle:MenuBuilder:createSideDashboardMenu',                      {                          'template': 'CyclogramFrontendBundle:Knp:knp_side_menu.html.twig',                           'currentClass': 'active', 'dashboardClass': 'branded'})                   }}              {% else %}                  {{ knp_menu_render('CyclogramFrontendBundle:MenuBuilder:createSideDashboardMenu',                      {                          'template': 'CyclogramFrontendBundle:Knp:knp_side_menu.html.twig',                           'currentClass': 'active'})                   }}              {% endif %}    {#             <ul class="left_menu">#}    {#               <li><a href="#" class="icon_dashboard active">Dashboard <span class="notice">5</span></a></li>#}    {#               <li class="first"><a class="icon_survey" href="#">Survey</a></li>#}    {#               <li><a class="icon_activities" href="#">Activities</a></li>#}    {#               <li><a class="icon_measurments" href="#">Measurements</a></li>#}    {#               <li><a class="icon_treatment" href="#">Treatment</a></li>#}    {#             </ul>#}              </div>            </div>            <div class="panel_right">              <div class="dashboard_title main {% if studyCode != 'kah' %}branded{% endif %}">                <h1>{% trans from "dashboard" %}txt_dashboard{% endtrans %}</h1>                <p>{% trans from "dashboard" %}dashboard_subtitle{% endtrans %}</p>              </div>              <div class="dashboard_panel">                {% if confirm_email == false %}                <p class="notice green"><span>{{ email_alert| raw }}</span> <a href="{{path('_main',{'sendMail' :  true})}}">{% trans from "dashboard" %}send_again{% endtrans %}</a></p>                {% endif %}                {% if confirmed is defined %}                <p class="notice green"><span>{{ confirmed }}</span></p>                {% endif %}                <div class="choices_box">                  <h2>{% trans from "dashboard" %}do_it_now{% endtrans %}<span> ({{ interventioncount }})</span></h2>                  {% if interventioncount > 0 %}                  {% for intervention in interventions %}                  <div class="featured_box">                    <div class="banner_box"><span class="icon_1"><span></span></span><img                        title=""                        alt=""                        src="{{ intervention.logo }}"                        height="126"                        width="126"></div>                    <div class="text_box">                             <h4>{{ intervention.title  | raw }}</h4>                            <p>{{ intervention.content | raw }}</p>                    </div>                    <div class="mobile featured_box_s_desc">                      <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit,                        sed diam nonummy nibh euismod tincidunt</p>                    </div>                        <div class="button_box"> <a href="{{ intervention.url }}"><span>{% trans from "dashboard" %}do_it{% endtrans %}</span></a> </div>                  </div>                  {% endfor %}                  {% else %}                  <div class="featured_box">                        <h2><br><span></span></h2><h2><span>{% trans from "dashboard" %}no_do_it{% endtrans %}  <br></span></h2><p><br></p>                <div class="mobile featured_box_s_desc">                  <p>{% trans from "dashboard" %}no_do_it{% endtrans %}</p>                </div>              </div>              {% endif %}                </div>{#                 <div class="act_wrapper">#}{#                   <div class="activity_box left">#}{#                     <h4>{% trans from "dashboard" %}past_activity{% endtrans %}</h4>#}{#                     <ul>#}{#                         {% for message in actions %}#}{#                         <li class="{{ message.class }}">#}{#                             <p>{{ message.activity }}</p>#}{#                         </li>#}{#                         {% endfor %}#}{#                     </ul>#}{#                     <a class="more" href="#">{% trans from "dashboard" %}view_more{% endtrans %}</a> </div>#}{#                   <div class="sep"></div>#}{#                   <div class="activity_box right">#}{#                     <h4>{% trans from "dashboard" %}what_next{% endtrans %}</h4>#}{#                     <ul>#}{#                         {% for help in assistance %}#}{#                             <li class="{{ help.class }}"><a href="#"><span>{{ help.info }}</span></a></li>#}{#                         {% endfor %}#}{#                     </ul>#}{#                     <a class="more" href="#">{% trans from "dashboard" %}view_more{% endtrans %}</a> </div>#}{#                 </div>#}              </div>            </div>          </div>        </div>        {% include '::footer.html.twig' %}        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>        <script>window.jQuery || document.write('<script src={{ asset('js/vendor/jquery-1.9.1.min.js') }}><\/script>')</script>        <script src= {{ asset ('js/plugins.js') }}></script>        <script src= {{ asset ('js/main.js') }}></script>    {% endblock content %}