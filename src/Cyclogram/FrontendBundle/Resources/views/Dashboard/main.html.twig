{%  extends '::base.html.twig' %}    {% block javascripts %}<script>   function fbLogout() {	  FB.getLoginStatus(function(response) {		  if (response.status === 'connected') {		    var uid = response.authResponse.userID;		    var accessToken = response.authResponse.accessToken;		    FB.logout(function (response) {		    });		  } 		 });  }    </script>    {% endblock javascripts %}    {% block body %}        {% if studyId == 1 %}            <body class="page_main" style="background: none; background-color: #878681 !important;">        {% else %}            <body class="page_main">        {% endif %}  {{ facebook_initialize({'async': true, 'xfbml': true}) }}    <div class="site_container">    {% include '::header.html.twig'%}      <div class="panel">        <div class="panel_left">          {% include '::profile_info.html.twig' %}          <div class="menu_cont">              {% if studyId == 1 %}                {{ knp_menu_render('CyclogramFrontendBundle:MenuBuilder:createSideDashboardMenuUnbranded',{'template': 'CyclogramFrontendBundle:Knp:knp_side_menu.html.twig', 'currentClass': 'active'}) }}              {% else %}                {{ knp_menu_render('CyclogramFrontendBundle:MenuBuilder:createSideDashboardMenu',{'template': 'CyclogramFrontendBundle:Knp:knp_side_menu.html.twig', 'currentClass': 'active'}) }}              {% endif %}{#             <ul class="left_menu">#}{#               <li><a href="#" class="icon_dashboard active">Dashboard <span class="notice">5</span></a></li>#}{#               <li class="first"><a class="icon_survey" href="#">Survey</a></li>#}{#               <li><a class="icon_activities" href="#">Activities</a></li>#}{#               <li><a class="icon_measurments" href="#">Measurements</a></li>#}{#               <li><a class="icon_treatment" href="#">Treatment</a></li>#}{#             </ul>#}          </div>        </div>        <div class="panel_right">            {% set unbranded = "" %}            {% if studyId > 0 %}                {% set unbranded = "unbranded" %}            {% else %}                {% set unbranded = "branded" %}            {% endif %}          <div class="dashboard_title main {{ unbranded }}">            <h1>{% trans from "dashboard" %}txt_dashboard{% endtrans %}</h1>            <p>{% trans from "dashboard" %}dashboard_subtitle{% endtrans %}</p>          </div>          <div class="dashboard_panel">            {% if confirm_email == false %}            <p class="notice green"><span>{{ email_alert| raw }}</span></p>            {% endif %}            {% if confirmed is defined %}            <p class="notice green"><span>{{ confirmed }}</span></p>            {% endif %}            <div class="choices_box">              <h2>{% trans from "dashboard" %}do_it_now{% endtrans %}<span> ({{ surveycount }})</span></h2>{#               {% for text in surveys %}#}{#               <div class="featured_box">#}{#                 <div class="banner_box"><span class="{{ text.icon }}"><span></span></span><img#}{#                     title=""#}{#                     alt=""#}{#                     src="{{ text.image }}"#}{#                     height="126"#}{#                     width="126"></div>#}{#                 <div class="text_box"> #}{#                     <span>#}{#                         <h4>{{ text.title }}</h4>#}{#                         <p>{{ text.content }}</p>#}{#                     </span> #}{#                 </div>#}{#                 <div class="mobile featured_box_s_desc">#}{#                   <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit,#}{#                     sed diam nonummy nibh euismod tincidunt</p>#}{#                 </div>#}{#                 <div class="button_box"> <a href="#"><span>{% trans from "dashboard" %}do_it{% endtrans %}</span></a> </div>#}{#               </div>#}{#               {% endfor %}#}            </div>            <div class="act_wrapper">              <div class="activity_box left">                <h4>{% trans from "dashboard" %}past_activity{% endtrans %}</h4>                <ul>                    {% for message in actions %}                    <li class="{{ message.class }}">                        <p>{{ message.activity }}</p>                    </li>                    {% endfor %}                </ul>                <a class="more" href="#">{% trans from "dashboard" %}view_more{% endtrans %}</a> </div>              <div class="sep"></div>              <div class="activity_box right">                <h4>{% trans from "dashboard" %}what_next{% endtrans %}</h4>                <ul>                    {% for help in assistance %}                        <li class="{{ help.class }}"><a href="#"><span>{{ help.info }}</span></a></li>                    {% endfor %}                </ul>                <a class="more" href="#">{% trans from "dashboard" %}view_more{% endtrans %}</a> </div>            </div>          </div>        </div>      </div>    </div>    {% include '::footer.html.twig' %}    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>        <script>window.jQuery || document.write('<script src={{ asset('js/vendor/jquery-1.9.1.min.js') }}><\/script>')</script>        <script src= {{ asset ('js/plugins.js') }}></script>        <script src= {{ asset ('js/main.js') }}></script>    <script type="text/javascript">        $(document).ready(function(){            $('#facebook_logout').click(function()            {            	fbLogout();            	window.location=Routing.generate('_logout');            });        });    </script>   </body>{% endblock body %}